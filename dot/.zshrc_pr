# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...
alias l='ls -alh'
alias vi='vim'
alias dc='docker-compose'
alias dm='docker-machine'
alias ps='ps aux'
alias d='docker'

# Java Settings
export JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_66.jdk/Contents/Home

# Customize to your needs...
export PATH="/Users/kono/anaconda/bin:/usr/local/bin:/usr/local/sbin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin:/Users/kono/.rvm/bin:/Users/kono/bin"

# For CalVR
export CALVR_HOME=/Users/kono/git/CalVR/calvr
export CALVR_CONFIG_FILE=config.xml
export DYLD_LIBRARY_PATH=/Users/kono/git/CalVR/calvr/lib

# For GO
export GOPATH=~/git/go
export PATH=$PATH:$GOROOT/bin

export LC_ALL=en_US.UTF-8
